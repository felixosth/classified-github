<UserControl x:Class="REIDSearchAgent.SearchToolbar.Add.AddUsrMainUsrControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:REIDSearchAgent.SearchToolbar.Add"
             mc:Ignorable="d" Height="685.411" Width="1258.243" Margin="0, 0, 0, 10">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <ScrollViewer Margin="10,10,10,35" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
            <StackPanel x:Name="stackPanel" Orientation="Vertical">
            </StackPanel>
        </ScrollViewer>
        <Button x:Name="okBtn" Content="OK" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="okBtn_Click"/>
        <Button x:Name="cancelBtn" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="cancelBtn_Click"/>

    </Grid>
</UserControl>
