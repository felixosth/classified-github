<Window x:Class="TryggDRIFT_Configurator.PluginConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TryggDRIFT_Configurator"
        mc:Ignorable="d"
        Title="Plugin configuration" Height="526" Width="708" Loaded="Window_Loaded" Unloaded="Window_Unloaded" Icon="Resources/dashboard.ico">
    <Grid>
        <DockPanel x:Name="pluginConfigGrid" Margin="10,10,10,33"/>
        <Button x:Name="cancelBtn" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,8" VerticalAlignment="Bottom" Width="75" Click="cancelBtn_Click"/>
        <Button x:Name="saveBtn" Content="Save" HorizontalAlignment="Right" Margin="0,0,90,8" VerticalAlignment="Bottom" Width="75" Click="saveBtn_Click"/>
    </Grid>

</Window>
