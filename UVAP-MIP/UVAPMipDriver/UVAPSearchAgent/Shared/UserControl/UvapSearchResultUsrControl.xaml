<search:SearchResultUserControl x:Class="UVAPSearchAgent.Shared.UserControl.UvapSearchResultUsrControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:search="clr-namespace:VideoOS.Platform.Search;assembly=VideoOS.Platform"
             xmlns:local="clr-namespace:UVAPSearchAgent.Shared.UserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Viewbox Stretch="Fill">
        <Canvas x:Name="canvas">
            <Image x:Name="image" Source="/UVAPSearchAgent;component/Resources/Server.png" Width="{Binding ActualWidth, ElementName=canvas, Mode=OneWay}" Height="{Binding ActualHeight, ElementName=canvas, Mode=OneWay}" Stretch="Uniform" />
        </Canvas>
    </Viewbox>
</search:SearchResultUserControl>
