<Window x:Class="WinServLite2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinServLite2"
        mc:Ignorable="d" KeyDown="Window_KeyDown"
        Title="WinServLite 2.0" Height="900" Width="1600" Icon="Resources/rocket.ico" Closing="Window_Closing" Loaded="Window_Loaded">
    <TabControl x:Name="tabControl" Margin="4,2,4,4">
        <TabItem Header="Browser">
            <local:JobBrowser x:Name="jobBrowser"/>
        </TabItem>
    </TabControl>
</Window>
